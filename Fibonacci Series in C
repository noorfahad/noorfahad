#include <stdio.h>
int main()
{
    
    int i, num1,num2,num3 ;
    
    
    printf("Enter the number1 and number2: ");
    scanf("%d%d", &num1,&num2);
    
    printf("%d%d\n", num1, num2 );
    
    for (i=2; i<20; i++)
    
    {
    
    num3=num1+num2;
    
    printf("\n%d",num3);
    
    num1=num2;
    num2=num3;
    
    
    }
}
