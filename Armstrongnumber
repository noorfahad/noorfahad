#include<stdio.h>

int main()
{
    int temp,num,r;
    int sum = 0;
    
    printf("Enter the number ");
    scanf("%d", &num);
    
    temp=num;
    
    while (num>0)
    {
        r=num%10;
        sum= sum+(r*r*r);
        num=num/10;
    }
    
    if (temp==sum)
    
    {
        printf("The number is Armstrong");
        
    }
    else
    {
        printf("The number is not Armstrong");
    }
}
